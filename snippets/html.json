{
  "Breadcrumbs": {
    "prefix": "breadcrumbs",
    "body": [
      "<nav>",
      "  <p>",
      "    <a href=\"${1:/}\">Home</a>",
      "    <a href=\"${2:/products}\">Products</a>",
      "    <a href=\"${3:/details}\">${4:Product Details}</a>",
      "  </p>",
      "</nav>"
    ],
    "description": "Semantic HTML breadcrumbs navigation"
  },
  "Card": {
    "prefix": "card",
    "body": [
      "<article>",
      "  <header>",
      "    <h2>${1:Card Title}</h2>",
      "  </header>",
      "  <p>${2:Card content goes here providing key information to the user.}</p>",
      "  <footer>",
      "    <a href=\"${3:#}\">${4:Action Link}</a>",
      "  </footer>",
      "</article>"
    ],
    "description": "Semantic HTML card component"
  },
  "Card List": {
    "prefix": "cardlist",
    "body": [
      "<ol>",
      "  <li>",
      "    <article>",
      "      <header>",
      "        <h2>${1:First Card Title}</h2>",
      "      </header>",
      "      <p>${2:This card contains important information related to the first item.}</p>",
      "      <footer>",
      "        <a href=\"${3:#}\">${4:View Details}</a>",
      "      </footer>",
      "    </article>",
      "  </li>",
      "  <li>",
      "    <article>",
      "      <header>",
      "        <h2>${5:Second Card Title}</h2>",
      "      </header>",
      "      <p>${6:This card contains important information related to the second item.}</p>",
      "      <footer>",
      "        <a href=\"${7:#}\">${8:View Details}</a>",
      "      </footer>",
      "    </article>",
      "  </li>",
      "</ol>"
    ],
    "description": "Semantic HTML list of cards"
  },
  "Checkbox List": {
    "prefix": "checkboxlist",
    "body": [
      "<fieldset>",
      "  <legend>${1:Options}</legend>",
      "  <p>",
      "    <input type=\"checkbox\" id=\"${2:option1}\" />",
      "    <label for=\"${2:option1}\">${3:First Option}</label>",
      "  </p>",
      "  <p>",
      "    <input type=\"checkbox\" id=\"${4:option2}\" />",
      "    <label for=\"${4:option2}\">${5:Second Option}</label>",
      "  </p>",
      "  <p>",
      "    <input type=\"checkbox\" id=\"${6:option3}\" />",
      "    <label for=\"${6:option3}\">${7:Third Option}</label>",
      "  </p>",
      "</fieldset>"
    ],
    "description": "Semantic HTML checkbox list with fieldset"
  },
  "Data With Labels": {
    "prefix": "datalist",
    "body": [
      "<h2>${1:Item Details}</h2>",
      "<dl>",
      "  <dt>${2:Property 1}</dt>",
      "  <dd>${3:Value 1}</dd>",
      "</dl>",
      "<dl>",
      "  <dt>${4:Property 2}</dt>",
      "  <dd>${5:Value 2}</dd>",
      "</dl>",
      "<dl>",
      "  <dt>${6:Property 3}</dt>",
      "  <dd>${7:Value 3}</dd>",
      "</dl>"
    ],
    "description": "Semantic HTML definition list for key-value data"
  },
  "Form Basic": {
    "prefix": "formbasic",
    "body": [
      "<h2>${1:Contact Form}</h2>",
      "<form action=\"${2:#}\" method=\"${3:post}\">",
      "  <p>",
      "    <label for=\"firstname\">First Name</label>",
      "    <input type=\"text\" id=\"firstname\" name=\"firstname\" />",
      "  </p>",
      "  <p>",
      "    <label for=\"lastname\">Last Name</label>",
      "    <input type=\"text\" id=\"lastname\" name=\"lastname\" />",
      "  </p>",
      "  <p>",
      "    <label for=\"email\">Email</label>",
      "    <input type=\"email\" id=\"email\" name=\"email\" />",
      "  </p>",
      "  <p>",
      "    <button type=\"submit\">${4:Submit}</button>",
      "  </p>",
      "</form>"
    ],
    "description": "Basic semantic HTML form"
  },
  "Form Validation Inline": {
    "prefix": "formvalidation",
    "body": [
      "<h2>${1:Contact Form with Validation}</h2>",
      "<form action=\"${2:#}\" method=\"${3:post}\">",
      "  <p>",
      "    <label for=\"firstname\">First Name (required)</label>",
      "    <input type=\"text\" id=\"firstname\" name=\"firstname\" aria-describedby=\"firstname_error\" required />",
      "    <span id=\"firstname_error\" aria-live=\"assertive\">${4:First name is required.}</span>",
      "  </p>",
      "  <p>",
      "    <label for=\"lastname\">Last Name (required)</label>",
      "    <input type=\"text\" id=\"lastname\" name=\"lastname\" aria-describedby=\"lastname_error\" required />",
      "    <span id=\"lastname_error\" aria-live=\"assertive\">${5:Last name is required.}</span>",
      "  </p>",
      "  <p>",
      "    <label for=\"email\">Email (required)</label>",
      "    <input type=\"email\" id=\"email\" name=\"email\" aria-describedby=\"email_error\" required />",
      "    <span id=\"email_error\" aria-live=\"assertive\">${6:Valid email is required.}</span>",
      "  </p>",
      "  <p>",
      "    <button type=\"submit\">${7:Submit}</button>",
      "  </p>",
      "</form>"
    ],
    "description": "Semantic HTML form with inline validation"
  },
  "Form Validation Summary": {
    "prefix": "formvalidationsummary",
    "body": [
      "<h2>${1:Contact Form with Validation Summary}</h2>",
      "<section role=\"alert\">",
      "  <h3>Please correct the following errors</h3>",
      "  <ul>",
      "    <li><a href=\"#firstname\" id=\"firstname_error\">${2:First name is required.}</a></li>",
      "    <li><a href=\"#lastname\" id=\"lastname_error\">${3:Last name is required.}</a></li>",
      "    <li><a href=\"#email\" id=\"email_error\">${4:Valid email is required.}</a></li>",
      "  </ul>",
      "</section>",
      "<form action=\"${5:#}\" method=\"${6:post}\">",
      "  <p>",
      "    <label for=\"firstname\">First Name (required)</label>",
      "    <input type=\"text\" id=\"firstname\" name=\"firstname\" aria-describedby=\"firstname_error\" required />",
      "  </p>",
      "  <p>",
      "    <label for=\"lastname\">Last Name (required)</label>",
      "    <input type=\"text\" id=\"lastname\" name=\"lastname\" aria-describedby=\"lastname_error\" required />",
      "  </p>",
      "  <p>",
      "    <label for=\"email\">Email (required)</label>",
      "    <input type=\"email\" id=\"email\" name=\"email\" aria-describedby=\"email_error\" required />",
      "  </p>",
      "  <p>",
      "    <button type=\"submit\">${7:Submit}</button>",
      "  </p>",
      "</form>"
    ],
    "description": "Semantic HTML form with validation summary"
  },
  "Login Form": {
    "prefix": "login",
    "body": [
      "<article>",
      "  <header>",
      "    <h2>Login</h2>",
      "  </header>",
      "  <form action=\"${1:#}\" method=\"${2:post}\">",
      "    <p>",
      "      <label for=\"username\">Username</label>",
      "      <input type=\"text\" id=\"username\" name=\"username\" autocomplete=\"username\" />",
      "    </p>",
      "    <p>",
      "      <label for=\"password\">Password</label>",
      "      <input type=\"password\" id=\"password\" name=\"password\" autocomplete=\"current-password\" />",
      "    </p>",
      "    <p>",
      "      <button type=\"submit\">Sign In</button>",
      "    </p>",
      "  </form>",
      "  <footer>",
      "    <a href=\"${3:/register}\">${4:Create Account}</a> | <a href=\"${5:/forgot-password}\">${6:Forgot Password}</a>",
      "  </footer>",
      "</article>"
    ],
    "description": "Semantic HTML login form"
  },
  "Navigation": {
    "prefix": "nav",
    "body": [
      "<nav>",
      "  <ul>",
      "    <li><a href=\"${1:/}\">Home</a></li>",
      "    <li><a href=\"${2:/about}\">About</a></li>",
      "    <li><a href=\"${3:/products}\">Products</a></li>",
      "    <li><a href=\"${4:/contact}\">Contact</a></li>",
      "  </ul>",
      "</nav>"
    ],
    "description": "Semantic HTML navigation menu"
  },
  "Accordion": {
    "prefix": "accordion",
    "body": [
      "<div class=\"accordion\">",
      "  <details>",
      "    <summary>${1:Section 1 Title}</summary>",
      "    <div class=\"accordion-content\">",
      "      <p>${2:Content for section 1.}</p>",
      "    </div>",
      "  </details>",
      "  <details>",
      "    <summary>${3:Section 2 Title}</summary>",
      "    <div class=\"accordion-content\">",
      "      <p>${4:Content for section 2.}</p>",
      "    </div>",
      "  </details>",
      "</div>"
    ],
    "description": "Semantic HTML accordion using details/summary"
  }
}
