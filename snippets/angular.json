{
  "Breadcrumbs": {
    "prefix": "breadcrumbs",
    "body": [
      "<nav>",
      "  <p>",
      "    <a [routerLink]=\"['${1:/}']\">Home</a>",
      "    <a [routerLink]=\"['${2:/products}']\">Products</a>",
      "    <a [routerLink]=\"['${3:/details}']\">${4:Product Details}</a>",
      "  </p>",
      "</nav>"
    ],
    "description": "Semantic Angular breadcrumbs navigation"
  },
  "Card": {
    "prefix": "card",
    "body": [
      "<article>",
      "  <header>",
      "    <h2>${1:Card Title}</h2>",
      "  </header>",
      "  <p>${2:Card content goes here providing key information to the user.}</p>",
      "  <footer>",
      "    <a [routerLink]=\"['${3:#}']\">${4:Action Link}</a>",
      "  </footer>",
      "</article>"
    ],
    "description": "Semantic Angular card component"
  },
  "Card List": {
    "prefix": "cardlist",
    "body": [
      "<ol>",
      "  <li>",
      "    <article>",
      "      <header>",
      "        <h2>${1:First Card Title}</h2>",
      "      </header>",
      "      <p>${2:This card contains important information related to the first item.}</p>",
      "      <footer>",
      "        <a [routerLink]=\"['${3:#}']\">${4:View Details}</a>",
      "      </footer>",
      "    </article>",
      "  </li>",
      "  <li>",
      "    <article>",
      "      <header>",
      "        <h2>${5:Second Card Title}</h2>",
      "      </header>",
      "      <p>${6:This card contains important information related to the second item.}</p>",
      "      <footer>",
      "        <a [routerLink]=\"['${7:#}']\">${8:View Details}</a>",
      "      </footer>",
      "    </article>",
      "  </li>",
      "</ol>"
    ],
    "description": "Semantic Angular list of cards"
  },
  "Checkbox List": {
    "prefix": "checkboxlist",
    "body": [
      "<fieldset>",
      "  <legend>${1:Options}</legend>",
      "  <p>",
      "    <input type=\"checkbox\" id=\"${2:option1}\" [(ngModel)]=\"${2:option1}\" name=\"${2:option1}\" />",
      "    <label for=\"${2:option1}\">${3:First Option}</label>",
      "  </p>",
      "  <p>",
      "    <input type=\"checkbox\" id=\"${4:option2}\" [(ngModel)]=\"${4:option2}\" name=\"${4:option2}\" />",
      "    <label for=\"${4:option2}\">${5:Second Option}</label>",
      "  </p>",
      "  <p>",
      "    <input type=\"checkbox\" id=\"${6:option3}\" [(ngModel)]=\"${6:option3}\" name=\"${6:option3}\" />",
      "    <label for=\"${6:option3}\">${7:Third Option}</label>",
      "  </p>",
      "</fieldset>"
    ],
    "description": "Semantic Angular checkbox list with fieldset"
  },
  "Data With Labels": {
    "prefix": "datalist",
    "body": [
      "<h2>${1:Item Details}</h2>",
      "<dl>",
      "  <dt>${2:Property 1}</dt>",
      "  <dd>{{ ${3:item}.${4:property1} }}</dd>",
      "</dl>",
      "<dl>",
      "  <dt>${5:Property 2}</dt>",
      "  <dd>{{ ${3:item}.${6:property2} }}</dd>",
      "</dl>",
      "<dl>",
      "  <dt>${7:Property 3}</dt>",
      "  <dd>{{ ${3:item}.${8:property3} }}</dd>",
      "</dl>"
    ],
    "description": "Semantic Angular definition list for key-value data"
  },
  "Form Basic": {
    "prefix": "formbasic",
    "body": [
      "<h2>${1:Contact Form}</h2>",
      "<form (ngSubmit)=\"${2:onSubmit()}\" #${3:contactForm}=\"ngForm\">",
      "  <p>",
      "    <label for=\"firstname\">First Name</label>",
      "    <input type=\"text\" id=\"firstname\" name=\"firstname\" [(ngModel)]=\"${4:model}.firstname\" />",
      "  </p>",
      "  <p>",
      "    <label for=\"lastname\">Last Name</label>",
      "    <input type=\"text\" id=\"lastname\" name=\"lastname\" [(ngModel)]=\"${4:model}.lastname\" />",
      "  </p>",
      "  <p>",
      "    <label for=\"email\">Email</label>",
      "    <input type=\"email\" id=\"email\" name=\"email\" [(ngModel)]=\"${4:model}.email\" />",
      "  </p>",
      "  <p>",
      "    <button type=\"submit\">${5:Submit}</button>",
      "  </p>",
      "</form>"
    ],
    "description": "Basic semantic Angular form"
  },
  "Form Validation Inline": {
    "prefix": "formvalidation",
    "body": [
      "<h2>${1:Contact Form with Validation}</h2>",
      "<form (ngSubmit)=\"${2:onSubmit()}\" #${3:contactForm}=\"ngForm\">",
      "  <p>",
      "    <label for=\"firstname\">First Name (required)</label>",
      "    <input type=\"text\" id=\"firstname\" name=\"firstname\" [(ngModel)]=\"${4:model}.firstname\" required #firstname=\"ngModel\" />",
      "    <span id=\"firstname_error\" aria-live=\"assertive\" *ngIf=\"firstname.invalid && (firstname.dirty || firstname.touched)\">${5:First name is required.}</span>",
      "  </p>",
      "  <p>",
      "    <label for=\"lastname\">Last Name (required)</label>",
      "    <input type=\"text\" id=\"lastname\" name=\"lastname\" [(ngModel)]=\"${4:model}.lastname\" required #lastname=\"ngModel\" />",
      "    <span id=\"lastname_error\" aria-live=\"assertive\" *ngIf=\"lastname.invalid && (lastname.dirty || lastname.touched)\">${6:Last name is required.}</span>",
      "  </p>",
      "  <p>",
      "    <label for=\"email\">Email (required)</label>",
      "    <input type=\"email\" id=\"email\" name=\"email\" [(ngModel)]=\"${4:model}.email\" required email #email=\"ngModel\" />",
      "    <span id=\"email_error\" aria-live=\"assertive\" *ngIf=\"email.invalid && (email.dirty || email.touched)\">${7:Valid email is required.}</span>",
      "  </p>",
      "  <p>",
      "    <button type=\"submit\" [disabled]=\"!${3:contactForm}.form.valid\">${8:Submit}</button>",
      "  </p>",
      "</form>"
    ],
    "description": "Semantic Angular form with inline validation"
  },
  "Form Validation Summary": {
    "prefix": "formvalidationsummary",
    "body": [
      "<h2>${1:Contact Form with Validation Summary}</h2>",
      "<section role=\"alert\" *ngIf=\"${2:formErrors}.length > 0\">",
      "  <h3>Please correct the following errors</h3>",
      "  <ul>",
      "    <li *ngFor=\"let error of ${2:formErrors}\">",
      "      <a [href]=\"'#' + error.field\">{{ error.message }}</a>",
      "    </li>",
      "  </ul>",
      "</section>",
      "<form (ngSubmit)=\"${3:onSubmit()}\" #${4:contactForm}=\"ngForm\">",
      "  <p>",
      "    <label for=\"firstname\">First Name (required)</label>",
      "    <input type=\"text\" id=\"firstname\" name=\"firstname\" [(ngModel)]=\"${5:model}.firstname\" required #firstname=\"ngModel\" />",
      "  </p>",
      "  <p>",
      "    <label for=\"lastname\">Last Name (required)</label>",
      "    <input type=\"text\" id=\"lastname\" name=\"lastname\" [(ngModel)]=\"${5:model}.lastname\" required #lastname=\"ngModel\" />",
      "  </p>",
      "  <p>",
      "    <label for=\"email\">Email (required)</label>",
      "    <input type=\"email\" id=\"email\" name=\"email\" [(ngModel)]=\"${5:model}.email\" required email #email=\"ngModel\" />",
      "  </p>",
      "  <p>",
      "    <button type=\"submit\" [disabled]=\"!${4:contactForm}.form.valid\">${6:Submit}</button>",
      "  </p>",
      "</form>"
    ],
    "description": "Semantic Angular form with validation summary"
  },
  "Login Form": {
    "prefix": "login",
    "body": [
      "<article>",
      "  <header>",
      "    <h2>Login</h2>",
      "  </header>",
      "  <form (ngSubmit)=\"${1:onSubmit()}\" #${2:loginForm}=\"ngForm\">",
      "    <p>",
      "      <label for=\"username\">Username</label>",
      "      <input type=\"text\" id=\"username\" name=\"username\" [(ngModel)]=\"${3:model}.username\" required />",
      "    </p>",
      "    <p>",
      "      <label for=\"password\">Password</label>",
      "      <input type=\"password\" id=\"password\" name=\"password\" [(ngModel)]=\"${3:model}.password\" required />",
      "    </p>",
      "    <p>",
      "      <button type=\"submit\" [disabled]=\"!${2:loginForm}.form.valid\">Sign In</button>",
      "    </p>",
      "  </form>",
      "  <footer>",
      "    <a [routerLink]=\"['${4:/register}']\">Create Account</a> | <a [routerLink]=\"['${5:/forgot-password}']\">Forgot Password</a>",
      "  </footer>",
      "</article>"
    ],
    "description": "Semantic Angular login form"
  },
  "Navigation": {
    "prefix": "nav",
    "body": [
      "<nav>",
      "  <ul>",
      "    <li><a [routerLink]=\"['${1:/}']\" routerLinkActive=\"active\" [routerLinkActiveOptions]=\"{exact: true}\">Home</a></li>",
      "    <li><a [routerLink]=\"['${2:/about}']\" routerLinkActive=\"active\">About</a></li>",
      "    <li><a [routerLink]=\"['${3:/products}']\" routerLinkActive=\"active\">Products</a></li>",
      "    <li><a [routerLink]=\"['${4:/contact}']\" routerLinkActive=\"active\">Contact</a></li>",
      "  </ul>",
      "</nav>"
    ],
    "description": "Semantic Angular navigation menu with RouterLink"
  },
  "Accordion": {
    "prefix": "accordion",
    "body": [
      "<div class=\"accordion\">",
      "  <details>",
      "    <summary>${1:Section 1 Title}</summary>",
      "    <div class=\"accordion-content\">",
      "      <p>${2:Content for section 1.}</p>",
      "    </div>",
      "  </details>",
      "  <details>",
      "    <summary>${3:Section 2 Title}</summary>",
      "    <div class=\"accordion-content\">",
      "      <p>${4:Content for section 2.}</p>",
      "    </div>",
      "  </details>",
      "</div>"
    ],
    "description": "Semantic Angular accordion using details/summary"
  }
}
